
CREATE TABLE testTBL2(
	id Number(4) PRIMARY KEY,
	userName NCHAR(3) not null,
	age Number(2),
	nation NCHAR(4) default '대한민국'
	
);

SELECT * FROM testTBL2 ORDER BY id DESC;
INSERT INTO testTBL2(id,userName,age,nation) 
